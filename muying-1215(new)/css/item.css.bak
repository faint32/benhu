.shaidan_a img {max-width:830px;
        max-height:600px; 
        scale:expression((this.offsetWidth > this.offsetHeight)?(this.style.width = this.offsetWidth >= 830 ? "830px" : "auto"):(this.style.height = this.offsetHeight >= 600 ? "600px" : "auto")); display:inline !important;border:1px #e3e3e3 solid; padding:4px; margin-top:5px;
}
